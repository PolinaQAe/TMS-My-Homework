import java.util.ArrayList;
import java.util.HashSet;
import java.util.List;

public class Main {
    public static void 11 task2(String[] args) {
        List<Integer> numbers = new ArrayList<>();
        // Заполнение коллекции numbers
        numbers.add(1);
        numbers.add(2);
        numbers.add(3);
        numbers.add(3);
        numbers.add(3);
        numbers.add(2);
        numbers.add(5);

        System.out.println("Исходная коллекция: " + numbers);

        HashSet<Integer> uniqueNumbers = new HashSet<>();
        HashSet<Integer> duplicateNumbers = new HashSet<>();

        for (Integer number : numbers) {
            if (!uniqueNumbers.add(number)) {
                duplicateNumbers.add(number);
            }
        }

        numbers.removeAll(duplicateNumbers);

        System.out.println("Результат: " + numbers);
    }
}
